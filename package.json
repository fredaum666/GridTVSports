{
  "name": "gridtvsports",
  "private": true,
  "version": "1.0.0",
  "description": "Multi-Sport Live Games & Sports Bar Mode Application",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon server.js",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "postinstall": "npm run build:prod",
    "start": "node server.js",
    "start:dev": "npm run build:dev && node server.js",
    "start:prod": "npm run build:prod && set NODE_ENV=production&& node server.js",
    "setup-db": "node setup-db.js",
    "test-db": "node test-db.js"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.11",
    "pg": "^8.16.3",
    "socket.io": "^4.8.1",
    "stripe": "^20.0.0",
    "terser-webpack-plugin": "^5.3.16",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
